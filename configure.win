echo "Building libRcpp.a in RcppSrc..."
R_HOME=`R RHOME`
CXX=`R CMD config CXX`
CC=`R CMD config CC`
cd RcppSrc
make RHOME=${R_HOME} -f Makefile.win
cd ..
if test \! -d inst
then
    mkdir inst
fi
cp RcppSrc/Rcpp-license.txt inst

echo "Building imslib.dll in src/imslib..."
cd src/imslib/
configure
make src/libims.la

mkdir -f $DPKG/libs
cp src/.libs/libims* $DPKG/libs/

ls -l src/.libs/
ls -l $DPKG/libs/

cd ../..



